module Practica_1_NP_tb();

// Variables
reg [3:0] Switch;
wire LED;

Practica_1_NP DUT(
	.Switch(Switch),
	.LED(LED)
);


initial 
	begin
	$display("Simulacion");
	Switch= 4'b0000;
	#10
	$stop;
	$display("Fin");
	$finish;
end

initial
		begin
			$monitor("Switch=%b, LED=%b", Switch, LED);
end

endmodule
	